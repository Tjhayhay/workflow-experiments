  "scripts": {
    "android": "react-native run-android --variant devdebug --appIdSuffix 'dev' --active-arch-only",
    "android-production": "react-native run-android --variant productiondebug --active-arch-only",
    "ios": "react-native run-ios --scheme 'Dev'",
    "iosProduction": "react-native run-ios --scheme 'Production'",
    "start": "react-native start",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint $(git diff --name-only HEAD | grep -E '\\.(js|jsx|ts|tsx)$' | xargs)",
    "resetDevEnv": "watchman watch-del-all && rm -rf node_modules/ && rm -rf $TMPDIR/react-packager-* && rm -rf ./android/build rm -rf ~/.rncache/ && npm install ",
    "resetIOS": "rm -rf ./ios/build && rm -rf ~/Library/Developer/Xcode/DerivedData && cd ios/ && rm -rf Pods/ && rm -rf Podfile.lock && cd .. && bundler exec arch -x86_64 pod install --project-directory=ios",
    "bump-version": "babel-node -- ./scripts/bumpVersion.js",
    "get-version": "babel-node -- ./scripts/getVersion.js",
    "postinstall": "patch-package",
    "devtools": "npx react-devtools",
    "compile-graphql-codegen": "graphql-codegen",
    "watch": "graphql-codegen -w"
  },
